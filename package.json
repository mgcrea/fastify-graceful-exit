{
  "name": "@mgcrea/fastify-graceful-exit",
  "version": "1.1.1",
  "description": "Graceful exit for your fastify application",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/fastify-session-prisma-store",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "fastify",
    "graceful",
    "exit"
  ],
  "scripts": {
    "start": "vitest",
    "dev": "vitest --watch",
    "build": "tsup --entry src/index.ts --format cjs,esm --sourcemap --dts --clean --entry src/bin/cli.ts",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "prettify": "prettier --write src/ test/",
    "typecheck": "tsc --noEmit",
    "spec": "vitest --run",
    "test": "npm run lint && npm run prettycheck && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "@mgcrea/eslint-config-node": "^0.12.12",
    "@mgcrea/fastify-session": "^2.4.0",
    "@prisma/client": "^5.21.0",
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.16.12",
    "benchmark": "^2.1.4",
    "eslint": "^9.12.0",
    "fastify": "^5.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  }
}
