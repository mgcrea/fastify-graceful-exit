{
  "name": "@mgcrea/fastify-graceful-exit",
  "version": "0.3.0",
  "description": "Graceful exit for your fastify application",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/fastify-graceful-exit",
  "license": "MIT",
  "access": "public",
  "scripts": {
    "start": "npm run spec -- --watch",
    "build": "rimraf lib/*; tsc --build",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "typecheck": "tsc --noEmit",
    "spec": "DEBUG=fastify-session*,prisma-query jest --runInBand",
    "test": "npm run lint && npm run prettycheck && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fastify",
    "graceful",
    "exit"
  ],
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "fastify": "^4.2.0",
    "jest": "^28.1.2",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.1"
  }
}
